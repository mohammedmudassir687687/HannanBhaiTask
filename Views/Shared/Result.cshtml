@model List<Employee>

<h1>Employee List</h1>

<table>
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.EmployeeID</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>
